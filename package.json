  "dependencies": {},
  "devDependencies": {
    "@types/node": "^8.10.11",
    "@types/webpack": "^4.1.4",
    "cross-env": "^5.1.4",
    "ts-loader": "^4.2.0",
    "ts-node": "^6.0.2",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2"
  },
  "scripts": {
